<template name="845a8ca4">
  <view class="_div data-v-f6962b48 wrap">
    <scroll-view scroll-y style="height:100%" class="_scroll-view data-v-f6962b48">
      <view class="_div data-v-f6962b48 swiper">
        <swiper indicator-dots="true" autoplay="true" interval="2000" duration="1000" indicator-active-color="#fff" wx:if="{{goodsDetail.supplierProductPictureVoList && goodsDetail.supplierProductPictureVoList.length>0}}" class="_swiper data-v-f6962b48">
          <block wx:key="index" key="{{index}}" wx:for="{{goodsDetail.supplierProductPictureVoList}}" wx:for-index="index" wx:for-item="val">
            <swiper-item class="_swiper-item data-v-f6962b48">
              <image src="{{val.imgUrl}}" class="_image data-v-f6962b48 slide-image" style="overflow:show"></image>
            </swiper-item>
          </block>
        </swiper>
        <image wx:if="{{goodsDetail.supplierProductPictureVoList && goodsDetail.supplierProductPictureVoList.length===0}}" src="{{goodsDetail.mainImgUrl}}" alt class="_img data-v-f6962b48"></image>
      </view>
      <view class="_div data-v-f6962b48 tit">
        <view class="_div data-v-f6962b48 ones">
          <view class="_p data-v-f6962b48">￥</view>
          <view class="_p data-v-f6962b48 price">{{goodsDetail.salesPrice}}</view>
          <view class="_s data-v-f6962b48 num">￥{{goodsDetail.vipPrice}}</view>
        </view>
        <view class="_p data-v-f6962b48 times" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}">分享赚：￥{{goodsDetail.memberDiscountPrice}}</view>
      </view>
      <view class="_div data-v-f6962b48 title">
        <view class="_div data-v-f6962b48 names">{{goodsDetail.title}}</view>
        <view class="_p data-v-f6962b48"><label class="_span data-v-f6962b48">快递包邮</label></view>
      </view>
      <view class="_ul data-v-f6962b48 list">
        <view class="_li data-v-f6962b48 lis"><label class="_span data-v-f6962b48">选择 规格</label>
          <view class="_div data-v-f6962b48 rights" bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}"><label class="_span data-v-f6962b48">颜色 尺码</label>
            <image src="/static/images/下一步.svg" alt class="_img data-v-f6962b48"></image>
          </view>
        </view>
        <view class="_li data-v-f6962b48">说明:{{goodsDetail.description}}</view>
      </view>
      <view class="_div data-v-f6962b48 footer"><button bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}" class="_button data-v-f6962b48">分享赚1.98</button> <button class="_button data-v-f6962b48">立即购买</button></view>
    </scroll-view>
  </view>
